<h1>Users</h1>

<table>
    <tr>
        <td>Name</td>
        <td>Email</td>
    </tr>
    <% @users.each do |user| %>
        <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td>
                <%= link_to "Show", "/users/#{user.id}" %>
                <%= link_to "Edit", "/users/#{user.id}/edit" %>
            </td>
        </tr>
    <% end %>
</table>

<%= link_to "Add a new user", "/users/new" %>